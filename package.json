{
    "name": "cx-lang",
    "version": "v0.8.0",
    "description": "A statically typed dynamic language",
    "keywords": [
        "cx",
        "cx-lang",
        "parser",
        "compiler",
        "transpiler",
        "ast",
        "javascript",
        "c++",
        "cpp"
    ],
