    "devDependencies": {
        "pegjs-dev": "0.10.0-156",
        "eslint": "4.6.x",
        "eslint-config-futagozaryuu": "3.5.x",
        "babel-register": "7.0.0-alpha.20",
        "babel-core": "7.0.0-alpha.20",
