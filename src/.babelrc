{

  "babelrc": false,
  "sourceMaps": true,

  "plugins": [

    [ "resolver", { "resolveDirs": [ "src" ] } ],
    "transform-es2015-modules-commonjs",

    "syntax-trailing-function-commas",
    [ "transform-async-to-module-method", {

        "module": "bluebird",
        "method": "coroutine"

    } ]

  ],

  "presets": [

    "es2016",
    "stage-0"

  ]

}
